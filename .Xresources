! X11 Generic Things
Xcursor.theme: whiteglass
Xcursor.size:  22
xterm*termName: xterm
xterm*locale: true

! Font stuff
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

! Color Theme (Dracula) https://github.com/dracula/xresources/blob/master/Xresources
*.foreground:     #F8F8F2
*.background:     #282A36
*.cursorColor:    #61AFEF
*.highlightColor: #3A3F4B
*.color0:         #000000
*.color8:         #4D4D4D
*.color1:         #FF5555
*.color9:         #FF6E67
*.color2:         #50FA7B
*.color10:        #5AF78E
*.color3:         #F1FA8C
*.color11:        #F4F99D
*.color4:         #BD93F9
*.color12:        #CAA9FA
*.color5:         #FF79C6
*.color13:        #FF92D0
*.color6:         #8BE9FD
*.color14:        #9AEDFE
*.color7:         #BFBFBF
*.color15:        #E6E6E6

